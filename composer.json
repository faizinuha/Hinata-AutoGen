{
    "name": "hinata/hikari-autogen",
    "description": "Tools Easy Create File or Laravel",
    "type": "library",
    "version": "v1.0.0",
    "homepage": "https://github.com/faizinuha/Leoe-AutoGen.git",
    "keywords": [
        "laravel",
        "autogen",
        "file",
        "create"
    ],
    "support": {
        "issues": "https://github.com/faizinuha/Leoe-AutoGen/issues"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/faizinuha/Leoe-AutoGen.git",
            "options": {
                "no-api": true
            }
        }
    ],
    "prefer-stable": true,
    "prefer-lowest": false,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hinata\\HikariAutogen\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Zaky-D"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/filesystem": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/mail": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/broadcasting": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^10.0",
        "mockery/mockery": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "squizlabs/php_codesniffer": "^3.0",
        "pestphp/pest": "^1.0"
    },
    
    "scripts": {
        "test": [
            "phpunit"
        ],
        "lint": [
            "phpcs --standard=PSR12 src tests"
        ],
        "fix": [
            "php-cs-fixer fix src tests"
        ],
        "post-autoload-dump": [
            "@php artisan vendor:publish --tag=autogen --force"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=autogen --force"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=autogen --force"
        ],
        "post-create-project-cmd": [
            "@php artisan vendor:publish --tag=autogen --force"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hinata\\HikariAutogen\\Providers\\ConsoleServiceProvider"
            ]
        }
    }
}
